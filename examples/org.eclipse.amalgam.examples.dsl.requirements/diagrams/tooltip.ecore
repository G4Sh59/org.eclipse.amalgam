<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="tooltip"
    nsURI="http://www.eclipse.org/2008/tooltip" nsPrefix="tooltip">
  <eClassifiers xsi:type="ecore:EClass" name="Tooltip">
    <eStructuralFeatures xsi:type="ecore:EReference" name="editpolicy" eType="ecore:EClass http://www.eclipse.org/gmf/2008/GenModel#//CustomBehaviour"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//DisplayElement" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subject" eType="ecore:EClass http://www.eclipse.org/emf/2002/GenModel#//GenClass"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DisplayElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="feature" eType="ecore:EClass http://www.eclipse.org/emf/2002/GenModel#//GenFeature"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="label" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Model">
    <eStructuralFeatures xsi:type="ecore:EReference" name="tooltips" upperBound="-1"
        eType="#//Tooltip" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
